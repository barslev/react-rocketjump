{
  "name": "react-rocketjump",
  "version": "0.8.1",
  "description": "Rocketjump your react! Handle state and side effects with classy",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "repository": "https://github.com/inmagik/react-rocketjump",
  "authors": [
    "Giovanni Fumagalli <giovanni.fumagalli@inmagik.com>",
    "Mauro Bianchi <mauro.bianchi@inmagik.com>",
    "Alberto Osio <osioalberto@gmail.com>"
  ],
  "scripts": {
    "dev": "webpack-dev-server",
    "server": "json-server --watch db.json --port 3004 --delay 500",
    "format": "prettier --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "codecov": "jest --coverage && codecov",
    "prebuild": "rimraf lib && node scripts/make-plugin-defs.js",
    "build": "rollup -c"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inmagik/react-rocketjump/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "state",
    "side effects",
    "rocketjump"
  ],
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0",
    "object-path-immutable": "^3.0.2",
    "query-string": "^6.7.0",
    "rocketjump-core": "^1.3.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "9.x",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.5.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "json-server": "^0.14.2",
    "memoize-one": "^5.0.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "react": "^16.9.0-alpha.0",
    "react-dom": "^16.9.0-alpha.0",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.9.0-alpha.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.5",
    "rollup-plugin-babel": "^4.3.2",
    "style-loader": "^0.23.1",
    "superagent": "^5.0.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
